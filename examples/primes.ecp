IMPORT "math"

SUBROUTINE isPrime(n: Integer)
    IF n = 1 THEN
        RETURN False
    ENDIF
    found := False
    FOR i := 2 TO Integer(SQRT(n)) + 1
        IF n MOD i = 0 AND i != n THEN
            found := True
            BREAK
        ENDIF
    ENDFOR
    
    RETURN NOT found
ENDSUBROUTINE

FOR i := 1 TO 100
    IF isPrime(i) THEN
        OUTPUT i
    ENDIF
ENDFOR
